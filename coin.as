package  {		import flash.display.MovieClip;	import flash.events.*;		public class coin extends MovieClip {		var moveItem:int;				public function coin() {			this.width = 30;			this.height = 30;			moveItem = 4;			addEventListener(Event.ENTER_FRAME, coinFrame);		}		public function coinFrame(event:Event):void{			this.x -= moveItem;			if(this.x < -192){				delete this;			}		}	}	}