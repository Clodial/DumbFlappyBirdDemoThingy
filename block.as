package  {		import flash.display.MovieClip;	import flash.events.*;		public class block extends MovieClip {		var moveItem:int;				public function block() {			this.width = 30;			this.height = 30;			moveItem = 4;			addEventListener(Event.ENTER_FRAME, blockFrame);		}		public function blockFrame(event:Event):void{			this.x -= moveItem;			if(this.x < -192){				delete this;			}		}	}	}